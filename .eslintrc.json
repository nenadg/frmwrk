{
  "root": true,
  "extends": ["eslint:recommended", "plugin:import/errors", "plugin:import/warnings"],
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-unused-expressions": "warn",
    "no-unused-vars": "warn"
  }
}
